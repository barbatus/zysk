{
  "extends": "@zysk/typescript-config/react.json",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "#/*": [
        "./*"
      ]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "reset.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ]
}
