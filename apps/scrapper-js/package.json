{
  "name": "@zysk/scrapper-api",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev:build": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "dev": "npm-run-all --parallel dev:build start",
    "start": "nodemon dist/server.js --watch",
    "lint": "eslint . --ext .ts --color",
    "lint:fix": "pnpm lint --fix"
  },
  "dependencies": {
    "@bull-board/api": "^6.9.6",
    "@bull-board/fastify": "^6.9.6",
    "@zysk/services": "workspace:*",
    "bullmq": "^5.53.1",
    "dotenv": "^16.4.5",
    "fastify": "^5.3.1",
    "ioredis": "^5.6.1",
    "turndown": "^7.1.2",
    "zod": "catalog:zod",
    "@zysk/scrapper": "workspace:*"
  },
  "devDependencies": {
    "@types/turndown": "^5.0.4",
    "@types/user-agents": "^1.0.4",
    "@zysk/eslint-config": "workspace:*",
    "@zysk/typescript-config": "workspace:*",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "tsc-alias": "^1.8.16",
    "typescript": "catalog:typescript"
  }
}
